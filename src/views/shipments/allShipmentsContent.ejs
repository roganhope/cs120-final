

<h1 class="page-title"><%= pageTitle %></h1>
   

<div class="button-container">
    <button type="submit"> <a href="/shipment/new"> New Shipment</a>

    </button>
</div>

<table class="full-width-table">
    <thead>
        <tr>
            <th>Invoice Number</th>
            <th>Vendor</th>
            <th>Order Date</th>
            <th>Expected Date</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        <% shipmentData.forEach(shipment => { %>
            <a href="/shipment/<%= shipment._id %>">
                <tr>
                    <td><a href="/shipment/<%= shipment._id %>"><%= shipment.invoiceno %></a></td>
                    <td><%= shipment.shipmentVendor %></td>
                    <td><%= shipment.orderDate %></td>
                    <td><%= shipment.expectedDate %></td>
                    <td><%= shipment.total %></td>
                </tr>
            </a>
        <% }); %>
    </tbody>
</table>
