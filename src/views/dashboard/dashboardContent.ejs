        <div class="highlight-card temp">
            <p class="highlight-number"><%= totalSales %></p>
            <p>Total Sales</p>
        </div>
        <div class="highlight-card temp">
            <p class="highlight-number"><%= totalInventory %></p>
            <p>Total Inventory</p>
        </div>
    </div>
</div>

