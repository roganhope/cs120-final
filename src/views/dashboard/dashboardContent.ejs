<div class="section-container">
    <div class="user-info">
        <p>Welcome, <%= userData.displayName %></p>
        <p>Email: <%= userData.email %></p>
    </div>
</div>

<div class="section-container">
    <div class="highlights temp">
        <div class="highlight-card temp">
            <p class="highlight-number"><%= totalClients %></p>
